<Application x:Class="WeMove_Mvvm.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WeMove_Mvvm">
    <Application.Resources>
        <ResourceDictionary>
            <local:ServiceLocator x:Key="Locator"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/UiCore/ValueConverters/ValueConverter.xaml"/>
                <ResourceDictionary Source="/Themes/DefaultStyles.xaml"/>
                <ResourceDictionary Source="/Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
